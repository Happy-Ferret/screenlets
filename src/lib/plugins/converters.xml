<xmlconverters>
<converter name="temperature">
	<unit name="celsius">
		<ratio>1</ratio>
		<offset>0</offset>
	</unit>
	<unit name="fahrenheit">
		<ratio>1.8</ratio>
		<offset>32</offset>
	</unit>
	<unit name="kelvin">
		<ratio>1</ratio>
		<offset>273.15</offset>
	</unit>
</converter>
<converter name="length">
	<unit name="m">
		<ratio>1</ratio>
		<offset>0</offset>
	</unit>
	<unit name="dm">
		<ratio>10</ratio>
		<offset>0</offset>
	</unit>
	<unit name="cm">
		<ratio>100</ratio>
		<offset>0</offset>
	</unit>
	<unit name="mm">
		<ratio>1000</ratio>
		<offset>0</offset>
	</unit>
</converter>
</xmlconverters>
